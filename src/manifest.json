{
    "manifest_version": 3,
    "name": "Catinator",
    "description": "Replaces all images on all pages to cat images",
    "version": "1.0",
    "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  }